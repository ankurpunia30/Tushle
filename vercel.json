{
  "version": 2,
  "name": "tushle-dashboard",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python@4.2.1"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/index.html"
    }
  ]
}